{
	"name": "digitalzenworks/consumer-provider",
	"authors":
	[
		{
			"name": "James John McGuire",
			"email": "jamesjohnmcguire@gmail.com"
		}
	],
	"description": "A package providing an interface to allow a provider object to feed a consumer object.",
	"funding":
	[
		{
			"type": "github",
			"url": "https://github.com/sponsors/jamesjohnmcguire"
		}
	],
	"homepage": "https://github.com/jamesjohnmcguire/consumer-provider/",
	"keywords": [ "consumer", "provider", "pattern" ],
	"license": "MIT",
	"support":
	{
		"issues": "https://github.com/jamesjohnmcguire/consumer-provider/issues"
	},
	"autoload":
	{
		"classmap":
		[
			"SourceCode/"
		],
		"psr-4":
		{
			"digitalzenworks\\ConsumerProvider\\": "SourceCode/"
		}
	},
	"bin":
	[
		"dzwcp-process"
	],
	"config":
	{
		"allow-plugins":
		{
			"dealerdirect/phpcodesniffer-composer-installer": true
		},
		"optimize-autoloader": true,
		"sort-packages": true,
		"vendor-dir": "SourceCode/vendor"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/jamesjohnmcguire/consumer-provider"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"type": "library",
	"require": {},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "*",
		"php-parallel-lint/php-parallel-lint": "^1.2.0",
		"phpcompatibility/php-compatibility": "*",
		"phpcsstandards/phpcsdevtools": "^1.0",
		"phpstan/phpstan": "^1.2",
		"phpunit/phpunit": "^9.5",
		"squizlabs/php_codesniffer": "*"
	}
}
